{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch site",
      "program": "${workspaceRoot}/bin/www",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "appinsights,oss-redis,oss-github,oss-initialize,oss-redis-cross-org,appinsights,azureossportal,health",
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch site (SUDO FORCE)",
      "program": "${workspaceRoot}/bin/www",
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "appinsights,oss-github,oss-initialize,oss-redis-cross-org,appinsights,azureossportal",
        "DEBUG_GITHUB_PORTAL_SUDO_FORCE": "1",
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch site (SUDO OFF)",
      "console": "integratedTerminal",
      "program": "${workspaceRoot}/bin/www",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "appinsights,oss-github,oss-initialize,oss-redis-cross-org,appinsights,azureossportal",
        "DEBUG_GITHUB_PORTAL_SUDO_OFF": "1",
        "DEBUG_GITHUB_ORG_SUDO_OFF": "1",
        "NODE_TLS_REJECT_UNAUTHORIZED": "0"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Reports",
      "program": "${workspaceRoot}/jobs/reports/index.js",
      "cwd": "${workspaceRoot}",
      "runtimeArgs": [
        "--max-old-space-size=4096"
      ],
      "env": {
        "NODE_ENV": "localhost",
        "DEBUG": "oss-redis-off,oss-github-off,oss-initialize,appinsights"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Job: Firehose",
      "program": "${workspaceRoot}/jobs/firehose/index.js",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "oss-redis,oss-github,oss-initialize,appinsights"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Job: Invitation Cleanup",
      "program": "${workspaceRoot}/jobs/cleanupInvites/index.js",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "oss-redis,oss-github,oss-initialize,appinsights"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Job: Permissions",
      "program": "${workspaceRoot}/jobs/permissions/index.js",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "oss-redis,oss-github,oss-initialize,appinsights"
      }
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858
    }
  ]
}